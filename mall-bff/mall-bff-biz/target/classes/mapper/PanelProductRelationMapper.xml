<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.noinch.mall.biz.bff.dao.mapper.PanelProductRelationMapper">
    
    <!-- 根据板块ID查询关联记录 -->
    <select id="listPanelProductRelationByPanelId" resultType="com.noinch.mall.biz.bff.dao.entity.PanelProductRelationDO">
        select *
        from panel_product_relation_test
        where del_flag = '0'
          and panel_id = #{panelId}
    </select>
</mapper>